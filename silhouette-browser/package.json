{
  "name": "silhouette-browser-v5",
  "version": "5.3.0",
  "description": "Silhouette Browser - La plataforma de desarrollo m√°s avanzada con IDE, Sandbox, Docker y AI",
  "main": "main-process/app-manager/main.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development electron .",
    "start": "NODE_ENV=production electron .",
    "build": "npm run build-main && npm run build-renderer",
    "build-main": "echo 'Building main process...'",
    "build-renderer": "echo 'Building renderer process...'",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:linux": "npm run build && electron-builder --linux",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest",
    "test:integration": "playwright test",
    "omnipotent:install": "npm install @playwright/mcp anthropic openai langchain @langchain/openai @langchain/anthropic",
    "omnipotent:demo": "node omnipotent-system/demo.js",
    "omnipotent:test": "node omnipotent-system/demo.js",
    "omnipotent:dev": "NODE_ENV=development npm run omnipotent:demo"
  },
  "build": {
    "appId": "com.silhouette.browser",
    "productName": "Silhouette Browser",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main-process/**/*",
      "renderer-process/**/*",
      "shared-services/**/*",
      "browser-engine/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "playwright",
        "to": "playwright"
      }
    ],
    "mac": {
      "target": "dmg",
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "@monaco-editor/react": "^4.6.0",
    "@octokit/rest": "^20.0.0",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "chokidar": "^3.6.0",
    "cross-env": "^7.0.0",
    "dockerode": "^4.0.2",
    "electron-serve": "^2.0.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.3.0",
    "express": "^4.18.0",
    "http-proxy-middleware": "^2.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "monaco-editor": "^0.52.0",
    "node-pty": "^1.0.0",
    "playwright": "^1.47.2",
    "puppeteer": "^23.7.1",
    "simple-git": "^3.22.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "tar": "^7.4.3",
    "live-server": "^1.2.2",
    "browser-sync": "^3.0.3",
    "testcontainers": "^10.13.2",
    "http-server": "^14.1.1",
    "react-grid-layout": "^1.5.1",
    "react-resizable": "^3.0.4",
    "vm2": "^3.9.19",
    "ws": "^8.17.1",
    "xterm": "^5.3.0",
    "xterm-addon-attach": "^0.9.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0"
  },
  "devDependencies": {
    "electron": "^32.2.0",
    "electron-builder": "^25.1.8",
    "electron-devtools-installer": "^3.2.0",
    "jest": "^29.7.0",
    "playwright-chromium": "^1.47.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "html-webpack-plugin": "^5.6.0"
  },
  "keywords": [
    "browser",
    "ai",
    "automation",
    "agents",
    "silhouette",
    "chatgpt",
    "perplexity"
  ],
  "author": "MiniMax Agent",
  "license": "MIT"
}